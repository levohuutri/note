cd /tmp && wget https://master.dl.sourceforge.net/project/geoserver/GeoServer/2.19.6/extensions/geoserver-2.19.6-vectortiles-plugin.zip?viasf=1 && \
unzip geoserver-2.19.6-vectortiles-plugin.zip -d geoserver-2.19.6-vectortiles-plugin && \
mv /tmp/geoserver-2.19.6-vectortiles-plugin/* /usr/local/tomcat/webapps/geoserver/WEB-INF/lib
